        "email address",
        "value object",
        "doctrine"
    ],
    "support": {
        "issues": "https://github.com/nordic-alliance/email-address-doctrine/issues"
    },
    "homepage": "https://github.com/nordic-alliance/email-address-doctrine",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Serhii Diahovchenko",
            "email": "sergey.dyagovchenko@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php" : "~7.1",
        "doctrine/dbal" : "^2.6@dev",
        "nordic/email-address" : "^1.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=7.0",
        "mockery/mockery": "^1.2"
    },
    "autoload": {
        "psr-4": {
